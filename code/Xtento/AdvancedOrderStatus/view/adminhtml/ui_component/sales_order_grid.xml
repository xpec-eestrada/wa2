<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <container name="advancedOrderStatusGrid">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="component" xsi:type="string">Xtento_AdvancedOrderStatus/js/advancedOrderStatusGrid.min</item>
            </item>
        </argument>
    </container>
    <columns name="sales_order_columns">
        <column name="status_color" class="Xtento\AdvancedOrderStatus\Ui\Component\Listing\Column\StatusColor">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="label" xsi:type="string" translate="true">Status Color</item>
                    <item name="componentDisabled" xsi:type="boolean">true</item>
                    <item name="visible" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>
